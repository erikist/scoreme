<% if @authorized_account %>
	var navBar = "<%= render 'navigation' %>";
	$("#top-bar").replaceWith(navBar);
<% else %>
	$('#error_explanation').append("<div class='alert alert-danger' style='margin-bottom:5px'><a href='#' class='close' data-dismiss='alert' aria-label='close'>&times;</a><strong>Danger!</strong> Username or password do not match. </div>");
<% end %>
